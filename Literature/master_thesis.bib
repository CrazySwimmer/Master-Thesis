@Article{Grossman1980,
  author    = {Sanford J. Grossman and Joseph E. Stiglitz},
  title     = {On the Impossibility of Informationally Efficient Markets},
  issn      = {00028282},
  number    = {3},
  pages     = {393--408},
  url       = {http://www.jstor.org/stable/1805228},
  volume    = {70},
  comment   = {This seminal paper from Grossman and Stiglitz introduce the idea that, under cost-free information, a competitive economy cannot always be in equilibrium.
For the information to be cost-free, we need prices to reflect all information (ie. investors cannot make extra profit by gathering additional information about a company). In this case, investors are not incentivized to spend resources and accept a price as given.
However, if no investor is willing to gather additional information, how can the asset price fully reflect all available information? This is the Grossman-Stiglitz Paradox (not explicitely mentioned in the paper). Under cost-free information, only minimal to no trading happens as each investor has the same set of beliefs.
Under costly information, some investors will find attractive the option to dig information and make extra-profits while other investors will stay "uninformed". Under this assumption, beliefs won't always be the same and trading will happen (ie. an equilibrium can be found).},
  file      = {:Papers/Grossman Stigliz 1980.pdf:PDF},
  groups    = {Rational Expectation},
  journal   = {The American Economic Review},
  publisher = {American Economic Association},
  year      = {1980},
}

